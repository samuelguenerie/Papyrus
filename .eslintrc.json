{
  "env": {
    "node": true
  },
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-typescript/recommended",
    "@vue/eslint-config-prettier/skip-formatting"
  ],
  "overrides": [
    {
      "files": ["cypress/e2e/**/*.{cy,spec}.{js,ts,jsx,tsx}"],
      "extends": ["plugin:cypress/recommended"]
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  }
}
